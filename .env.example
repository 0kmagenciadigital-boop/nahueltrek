# Google Maps API Key (público - incluido en build)
VITE_GOOGLE_MAPS_API_KEY=

# Google OAuth Credentials (obtener de Google Cloud Console)
VITE_GOOGLE_CLIENT_ID=
VITE_GOOGLE_CLIENT_SECRET=

# Google Drive Folder ID (para almacenar imágenes)
VITE_GOOGLE_DRIVE_FOLDER_ID=

# Google Sheets IDs (para base de datos)
VITE_GOOGLE_SHEETS_ACTIVIDADES_ID=
VITE_GOOGLE_SHEETS_LUGARES_ID=

# Google Form URL (para reservaciones)
VITE_GOOGLE_FORM_URL=

# Instrucciones:
# 1. Copia este archivo como .env
# 2. Sigue las instrucciones en GOOGLE_INTEGRATION.md
# 3. Completa cada variable con los valores de tu Google Cloud Project
# 4. NUNCA subas el archivo .env a GitHub (ya está en .gitignore)
